// Temporary Prisma Schema for Client Generation
// Will update when connecting to your Home Server DB

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// Dummy model to allow prisma generate to work
model TempUser {
  id    Int    @id @default(autoincrement())
  email String
}
